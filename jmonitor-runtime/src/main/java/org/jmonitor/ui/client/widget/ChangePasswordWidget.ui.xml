<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>

  <g:HTMLPanel>

    <br />
    <table>
      <tr>
        <td>
          &nbsp;Current Password:
        </td>
        <td>
          <g:PasswordTextBox ui:field="currentPasswordTextBox" />
        </td>
      </tr>
      <tr>
        <td>
          &nbsp;New Password:
        </td>
        <td>
          <g:PasswordTextBox ui:field="newPasswordTextBox" />
        </td>
      </tr>
      <tr>
        <td>
          &nbsp;Confirm New Password:
        </td>
        <td>
          <g:PasswordTextBox ui:field="confirmNewPasswordTextBox" />
        </td>
      </tr>
      <tr>
        <td />
        <td>
          <g:Button text="Change Password" ui:field="changePasswordButton" />
          <g:Button text="Cancel" ui:field="cancelButton" />
        </td>
      </tr>
    </table>

  </g:HTMLPanel>

</ui:UiBinder>
